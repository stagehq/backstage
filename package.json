{
  "name": "backstage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "yarn run generate",
    "dev": "next dev",
    "prebuild": "yarn run generate",
    "build": "next build",
    "start": "next start",
    "start:ci": "INSECURE_AUTH=true yarn run start",
    "eslint": "eslint --cache --cache-location=node_modules/.cache/.eslintcache --fix",
    "lint": "yarn run eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier '**/*' --ignore-unknown --write",
    "generate": "yarn run generate:prisma && yarn run generate:schema && yarn run generate:graphql-codegen",
    "generate:prisma": "prisma generate",
    "generate:schema": "cross-env GENERATE=true cross-env DOTENV_CONFIG_PATH=.env.example ts-node --require dotenv/config --transpile-only -P pothos.tsconfig.json src/server/graphql/schema.ts",
    "generate:graphql-codegen": "graphql-codegen",
    "prisma:migrate": "yarn prisma migrate dev",
    "prisma:studio": "prisma studio",
    "test": "jest",
    "typecheck": "concurrently --raw yarn:typecheck:*",
    "typecheck:app": "tsc --noEmit",
    "typecheck:cypress": "tsc --noEmit -p cypress/tsconfig.json",
    "cypress:open": "cypress open",
    "prepare": "husky install",
    "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\""
  },
  "dependencies": {
    "@emoji-mart/data": "^1.0.6",
    "@emoji-mart/react": "^1.0.1",
    "@graphql-codegen/schema-ast": "^2.5.1",
    "@headlessui/react": "^1.6.1",
    "@heroicons/react": "^1.0.6",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@next/bundle-analyzer": "^13.0.6",
    "@pothos/core": "^3.10.1",
    "@pothos/plugin-prisma": "^3.6.1",
    "@pothos/plugin-relay": "^3.10.2",
    "@prisma/client": "^4.1.1",
    "@react-hook/resize-observer": "^1.2.6",
    "@splitbee/web": "^0.3.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-oauth2": "^1.4.11",
    "@urql/devtools": "^2.0.3",
    "@urql/exchange-graphcache": "^5.0.5",
    "apollo-server-micro": "^3.6.3",
    "autoprefixer": "^10.4.2",
    "aws-sdk": "^2.1152.0",
    "babel-runtime": "^6.26.0",
    "base-64": "^1.0.0",
    "cookie-session": "^2.0.0",
    "dotenv": "^16.0.0",
    "emoji-mart": "^5.2.2",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "immer": "^9.0.15",
    "intercept-stdout": "^0.1.2",
    "is-url": "^1.2.4",
    "kbar": "^0.1.0-beta.36",
    "micro": "^10.0.1",
    "next": "^13.0.6",
    "next-auth": "^4.3.3",
    "next-connect": "^0.13.0",
    "nexus": "^1.2.0",
    "nodemailer": "^6.7.5",
    "notion-client": "^6.12.0",
    "notion-types": "^6.11.0",
    "notion-utils": "^6.11.0",
    "postmark": "^3.0.1",
    "raw-body": "^2.5.1",
    "react": "^18.2.0",
    "react-cookie-consent": "^8.0.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.1",
    "react-hot-toast": "^2.2.0",
    "react-image-crop": "^10.0.2",
    "react-notion-x": "^6.12.5",
    "recoil": "^0.7.4",
    "slate": "^0.86.0",
    "slate-history": "^0.86.0",
    "slate-hyperscript": "^0.77.0",
    "slate-react": "^0.86.0",
    "slate-soft-break": "^0.9.0",
    "slug": "^8.2.2",
    "stripe": "^11.1.0",
    "tailwindcss": "^3.0.23",
    "urql": "^3.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@faker-js/faker": "^7.6.0",
    "@graphql-codegen/cli": "2.15.0",
    "@graphql-codegen/near-operation-file-preset": "^2.2.6",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-operations": "2.5.8",
    "@graphql-codegen/typescript-urql": "^3.5.3",
    "@graphql-codegen/typescript-urql-graphcache": "^2.2.6",
    "@netlify/plugin-nextjs": "^4.2.7",
    "@stripe/stripe-js": "^1.23.0",
    "@types/base-64": "^1.0.0",
    "@types/common-tags": "^1.8.1",
    "@types/cookie-session": "^2.0.44",
    "@types/is-url": "^1.2.30",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.10",
    "@types/passport": "^1.0.7",
    "@types/react": "^18.0.26",
    "@types/slug": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "common-tags": "1.8.2",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "cypress": "^11.2.0",
    "eslint": "^8.29.0",
    "eslint-config-next": "^13.0.6",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": ">=12.3.4",
    "next-prisma-plugin-webpack5": "^0.0.0-dripip",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prisma": "^4.1.1",
    "typescript": "^4.6.2"
  },
  "jest": {
    "setupFiles": [
      "dotenv/config"
    ],
    "setupFilesAfterEnv": [
      "./src/test/jest-setup.ts"
    ],
    "transform": {
      "\\.tsx?$": [
        "babel-jest",
        {
          "configFile": "./.babelrc.jest.js"
        }
      ]
    }
  },
  "prisma": {
    "schema": "src/server/db/schema.prisma"
  },
  "lint-staged": {
    "**/*.{.js,.jsx,.ts,.tsx}": [
      "eslint . --fix"
    ],
    "**/*": [
      "prettier --write --ignore-unknown ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
