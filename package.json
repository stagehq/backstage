    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": ">=12.3.4",
    "next-prisma-plugin-webpack5": "^0.0.0-dripip",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "prisma": "^4.1.1",
    "re-resizable": "^6.9.9",
    "react-icons": "^4.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "jest": {
    "setupFiles": [
      "dotenv/config"
    ],
    "setupFilesAfterEnv": [
      "./src/test/jest-setup.ts"
    ],
    "transform": {
      "\\.tsx?$": [
        "babel-jest",
        {
          "configFile": "./.babelrc.jest.js"
        }
      ]
    }
  },
  "prisma": {
    "schema": "src/server/db/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} src/test/seed/index.ts"
  },
  "lint-staged": {
    "**/*.{.js,.jsx,.ts,.tsx}": [
      "eslint . --fix"
    ],
    "**/*": [
      "prettier --write --ignore-unknown ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
